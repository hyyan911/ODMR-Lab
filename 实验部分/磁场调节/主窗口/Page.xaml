<odmr_lab:PageBase  xmlns:odmr_lab="clr-namespace:ODMR_Lab" x:Class="ODMR_Lab.磁场调节.DisplayPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ODMR_Lab.温度监测部分" xmlns:chart="clr-namespace:Controls.Charts;assembly=Controls" xmlns:control="clr-namespace:Controls;assembly=Controls"
      mc:Ignorable="d" 
      d:DesignHeight="600" d:DesignWidth="800" Margin="10"
      >
    <Grid.RowDefinitions>
        <RowDefinition Height="200"/>
        <RowDefinition Height="40"/>
        <RowDefinition/>
    </Grid.RowDefinitions>
    <Grid.ColumnDefinitions>
        <ColumnDefinition Width="200"/>
        <ColumnDefinition/>
    </Grid.ColumnDefinitions>
    <Grid Grid.Row="0" Margin="10" Grid.Column="1">
        <Grid.RowDefinitions>
            <RowDefinition Height="30"/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Label Content="磁场预测" Grid.ColumnSpan="5" HorizontalContentAlignment="Center" FontSize="13" FontWeight="Bold" VerticalContentAlignment="Center" Foreground="#FFFAFAFA"/>
        <Label Content="θ(度):" Grid.Row="1" Grid.Column="0" HorizontalContentAlignment="Center" FontSize="13" FontWeight="Bold" VerticalContentAlignment="Center" Foreground="#FFFAFAFA"/>
        <Label Content="φ(度):" Grid.Row="1" Grid.Column="2" HorizontalContentAlignment="Center" FontSize="13" FontWeight="Bold" VerticalContentAlignment="Center" Foreground="#FFFAFAFA"/>
        <Label Content="Z坐标:" Grid.Row="1" Grid.Column="4" HorizontalContentAlignment="Center" FontSize="13" FontWeight="Bold" VerticalContentAlignment="Center" Foreground="#FFFAFAFA"/>
        <TextBox x:Name="ThetaPre" Grid.Column="1" Grid.Row="1" Margin="5" BorderThickness="0" FontSize="15" Text="1" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Background="#FF383838" Foreground="#FFF0F0F0" CaretBrush="White"/>
        <TextBox x:Name="PhiPre" Grid.Column="3" Grid.Row="1" Margin="5" BorderThickness="0" FontSize="15" Text="1" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Background="#FF383838" Foreground="#FFF0F0F0" CaretBrush="White"/>
        <TextBox x:Name="ZHeight" Grid.Column="5" Grid.Row="1" Margin="5" BorderThickness="0" FontSize="15" Text="1" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Background="#FF383838" Foreground="#FFF0F0F0" CaretBrush="White"/>
        <Label Content="位移台X:" Grid.Row="2" Grid.Column="0" HorizontalContentAlignment="Center" FontSize="13" FontWeight="Bold" VerticalContentAlignment="Center" Foreground="#FFFAFAFA"/>
        <Label Content="位移台Y:" Grid.Row="2" Grid.Column="2" HorizontalContentAlignment="Center" FontSize="13" FontWeight="Bold" VerticalContentAlignment="Center" Foreground="#FFFAFAFA"/>
        <Label Content="位移台Z:" Grid.Row="2" Grid.Column="4" HorizontalContentAlignment="Center" FontSize="13" FontWeight="Bold" VerticalContentAlignment="Center" Foreground="#FFFAFAFA"/>
        <Label x:Name="XPre" Grid.Column="1" Grid.Row="2" Margin="5" BorderThickness="0" FontSize="15" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Background="{x:Null}" Foreground="#FFF0F0F0"/>
        <Label x:Name="YPre" Grid.Column="3" Grid.Row="2" Margin="5" BorderThickness="0" FontSize="15" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Background="{x:Null}" Foreground="#FFF0F0F0"/>
        <Label x:Name="ZPre" Grid.Column="5" Grid.Row="2" Margin="5" BorderThickness="0" FontSize="15" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Background="{x:Null}" Foreground="#FFF0F0F0"/>
        <Label Content="角度:" Grid.Row="3" Grid.Column="0" HorizontalContentAlignment="Center" FontSize="13" FontWeight="Bold" VerticalContentAlignment="Center" Foreground="#FFFAFAFA"/>
        <Label Content="磁场强度(G):" Grid.Row="3" Grid.Column="2" HorizontalContentAlignment="Center" FontSize="13" FontWeight="Bold" VerticalContentAlignment="Center" Foreground="#FFFAFAFA"/>
        <Label x:Name="AnglePre" Grid.Column="1" Grid.Row="3" Margin="5,5,5,5" BorderThickness="0" FontSize="15" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Background="{x:Null}" Foreground="#FFF0F0F0"/>
        <Label x:Name="BPre" Grid.Column="3" Grid.Row="3" Margin="5,5,5,5" BorderThickness="0" FontSize="15" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Background="{x:Null}" Foreground="#FFF0F0F0"/>
        <control:DecoratedButton Name="TemplateBtn" Grid.Column="4" Click="CalculatePredictField" Grid.Row="3" Margin="5" Text="计算" FontWeight="Thin" FontSize="14" Background="#FF383838" Foreground="#FFFAFAFA" MoveInColor="#FF424242" PressedColor="#FF39393A" MoveInForeground="#FFFAFAFA" PressedForeground="#FFFAFAFA"/>
        <control:DecoratedButton Grid.Column="5" Grid.Row="3" Margin="5" Text="移动" FontWeight="Thin" FontSize="14" Background="#FF383838" Foreground="#FFFAFAFA" MoveInColor="#FF424242" PressedColor="#FF39393A" MoveInForeground="#FFFAFAFA" PressedForeground="#FFFAFAFA"/>
    </Grid>
    <control:AutoScrollViewer Grid.Column="0" Grid.Row="0" Grid.RowSpan="3">
        <control:AutoScrollViewer.Content>
            <WrapPanel Name="ParamPage" Width="190" Orientation="Horizontal">
                <Label Content="磁铁参数" Width="190" HorizontalContentAlignment="Center" HorizontalAlignment="Stretch"  Margin="5" FontSize="13" FontWeight="Bold" VerticalContentAlignment="Center" Foreground="#FFFAFAFA" Height="40"/>
                <Label Content="长度（mm）:" Margin="5" HorizontalAlignment="Left" HorizontalContentAlignment="Center" FontSize="13" FontWeight="Bold" VerticalContentAlignment="Center" Foreground="#FFFAFAFA" Height="40"/>
                <TextBox Name="MLength" Width="70" BorderThickness="0" FontSize="15" Text="1" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Background="#FF383838" Foreground="#FFF0F0F0" CaretBrush="White" Margin="5,5,5,5"/>
                <Label Content="半径（mm）:" Margin="5" HorizontalAlignment="Left" HorizontalContentAlignment="Center" FontSize="13" FontWeight="Bold" VerticalContentAlignment="Center" Foreground="#FFFAFAFA" Height="40"/>
                <TextBox x:Name="MRadius" Width="70" BorderThickness="0" FontSize="15" Text="1" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Background="#FF383838" Foreground="#FFF0F0F0" CaretBrush="White" Margin="5,5,5,5"/>
                <Label Content="磁场强度系数:" Margin="5" HorizontalAlignment="Left" HorizontalContentAlignment="Center" FontSize="13" FontWeight="Bold" VerticalContentAlignment="Center" Foreground="#FFFAFAFA" Height="40"/>
                <TextBox Name="MIntensity" Width="70" BorderThickness="0" FontSize="15" Text="1" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Background="#FF383838" Foreground="#FFF0F0F0" CaretBrush="White" Margin="5,5,5,5"/>
                <Label Content="旋转台参数" Width="190" HorizontalContentAlignment="Center" FontSize="13" FontWeight="Bold" VerticalContentAlignment="Center" Foreground="#FFFAFAFA" Height="40"/>
                <Label Content="角度基准点(度):" Margin="5" HorizontalAlignment="Center" HorizontalContentAlignment="Center" FontSize="13" FontWeight="Bold" VerticalContentAlignment="Center" Foreground="#FFFAFAFA" Height="40"/>
                <TextBox x:Name="AngleStart" ToolTip="当圆柱形磁铁轴向沿X轴时角度位移台的度数" Margin="5" Width="60" BorderThickness="0" FontSize="15" Text="1" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Background="#FF383838" Foreground="#FFF0F0F0" CaretBrush="White"/>
                <Label Content="偏心修正参数" Width="190" HorizontalContentAlignment="Center" FontSize="13" FontWeight="Bold" VerticalContentAlignment="Center" Foreground="#FFFAFAFA" Height="40"/>
                <Label Content="X（mm）:" Margin="5" HorizontalAlignment="Left" HorizontalContentAlignment="Center" FontSize="13" FontWeight="Bold" VerticalContentAlignment="Center" Foreground="#FFFAFAFA" Height="40"/>
                <TextBox Name="OffsetX" IsReadOnly="True" Margin="5" Width="90" BorderThickness="0" FontSize="15" Text="1" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Background="#FF383838" Foreground="#FFF0F0F0" CaretBrush="White"/>
                <Label Content="Y（mm）:" Margin="5" HorizontalAlignment="Left" HorizontalContentAlignment="Center" FontSize="13" FontWeight="Bold" VerticalContentAlignment="Center" Foreground="#FFFAFAFA" Height="40"/>
                <TextBox Name="OffsetY" IsReadOnly="True" Width="90" Margin="5" BorderThickness="0" FontSize="15" Text="1" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Background="#FF383838" Foreground="#FFF0F0F0" CaretBrush="White"/>
                <control:DecoratedButton Text="偏心参数计算" Click="OffsetCalculate" Margin="23,5,5,5" FontWeight="Thin" FontSize="14" Background="#FF383838" Foreground="#FFFAFAFA" MoveInColor="#FF424242" PressedColor="#FF39393A" MoveInForeground="#FFFAFAFA" PressedForeground="#FFFAFAFA" Height="40" Width="142"/>
                <Label Content="轴范围限制" Width="190" HorizontalContentAlignment="Center" FontSize="13" FontWeight="Bold" VerticalContentAlignment="Center" Foreground="#FFFAFAFA" Height="40"/>

                <Label Content="X:" Margin="5" HorizontalAlignment="Left" HorizontalContentAlignment="Center" FontSize="13" FontWeight="Bold" VerticalContentAlignment="Center" Foreground="#FFFAFAFA" Height="40"/>
                <TextBox x:Name="XRangeLo" Margin="5" Width="50" BorderThickness="0" FontSize="15" Text="1" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Background="#FF383838" Foreground="#FFF0F0F0" CaretBrush="White"/>
                <Label Content="—" Margin="5" HorizontalAlignment="Left" HorizontalContentAlignment="Center" FontSize="13" FontWeight="Bold" VerticalContentAlignment="Center" Foreground="#FFFAFAFA" Height="40"/>
                <TextBox x:Name="XRangeHi" Margin="5" Width="50" BorderThickness="0" FontSize="15" Text="1" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Background="#FF383838" Foreground="#FFF0F0F0" CaretBrush="White"/>

                <Label Content="Y:" Margin="5" HorizontalAlignment="Left" HorizontalContentAlignment="Center" FontSize="13" FontWeight="Bold" VerticalContentAlignment="Center" Foreground="#FFFAFAFA" Height="40"/>
                <TextBox x:Name="YRangeLo" Margin="5" Width="50" BorderThickness="0" FontSize="15" Text="1" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Background="#FF383838" Foreground="#FFF0F0F0" CaretBrush="White"/>
                <Label Content="—" Margin="5" HorizontalAlignment="Left" HorizontalContentAlignment="Center" FontSize="13" FontWeight="Bold" VerticalContentAlignment="Center" Foreground="#FFFAFAFA" Height="40"/>
                <TextBox x:Name="YRangeHi" Margin="5" Width="50" BorderThickness="0" FontSize="15" Text="1" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Background="#FF383838" Foreground="#FFF0F0F0" CaretBrush="White"/>

                <Label Content="Z:" Margin="5" HorizontalAlignment="Left" HorizontalContentAlignment="Center" FontSize="13" FontWeight="Bold" VerticalContentAlignment="Center" Foreground="#FFFAFAFA" Height="40"/>
                <TextBox x:Name="ZRangeLo" Margin="5" Width="50" BorderThickness="0" FontSize="15" Text="1" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Background="#FF383838" Foreground="#FFF0F0F0" CaretBrush="White"/>
                <Label Content="—" Margin="5" HorizontalAlignment="Left" HorizontalContentAlignment="Center" FontSize="13" FontWeight="Bold" VerticalContentAlignment="Center" Foreground="#FFFAFAFA" Height="40"/>
                <TextBox x:Name="ZRangeHi" Margin="5" Width="50" BorderThickness="0" FontSize="15" Text="1" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Background="#FF383838" Foreground="#FFF0F0F0" CaretBrush="White"/>


                <Label Content="轴映射选项" Width="190" HorizontalContentAlignment="Center" FontSize="13" FontWeight="Bold" VerticalContentAlignment="Center" Foreground="#FFFAFAFA" Height="40"/>
                <Label Content="X:" Margin="5" HorizontalAlignment="Left" HorizontalContentAlignment="Center" FontSize="13" FontWeight="Bold" VerticalContentAlignment="Center" Foreground="#FFFAFAFA" Height="40"/>
                <control:ComboBox Name="XRelate" PanelWidth="100" ToolTip="与规定X轴对应的磁铁位移台轴" ImagePlace="Right" IconMargin="6" TextAreaRatio="0.8" Height="40" Width="120" Margin="5" BorderThickness="0" FontSize="15" Background="#FF383838" Foreground="#FFF0F0F0" IconSource="/图片资源/downArrow.png"/>
                <Label Content="Y:" Margin="5" HorizontalAlignment="Left" HorizontalContentAlignment="Center" FontSize="13" FontWeight="Bold" VerticalContentAlignment="Center" Foreground="#FFFAFAFA" Height="40"/>
                <control:ComboBox Name="YRelate" PanelWidth="100" ToolTip="与规定Y轴对应的磁铁位移台轴" ImagePlace="Right" IconMargin="6" TextAreaRatio="0.8" Height="40" Width="120" Margin="5" BorderThickness="0" FontSize="15" Background="#FF383838" Foreground="#FFF0F0F0" IconSource="/图片资源/downArrow.png"/>
                <Label Content="Z:" Margin="5" HorizontalAlignment="Left" HorizontalContentAlignment="Center" FontSize="13" FontWeight="Bold" VerticalContentAlignment="Center" Foreground="#FFFAFAFA" Height="40"/>
                <control:ComboBox Name="ZRelate" PanelWidth="100" ToolTip="与规定Z轴对应的磁铁位移台轴" ImagePlace="Right" IconMargin="6" TextAreaRatio="0.8" Height="40" Width="120" Margin="5" BorderThickness="0" FontSize="15" Background="#FF383838" Foreground="#FFF0F0F0" IconSource="/图片资源/downArrow.png"/>
                <Label Content="反向Z轴" Margin="5" HorizontalAlignment="Left" HorizontalContentAlignment="Center" FontSize="13" FontWeight="Bold" VerticalContentAlignment="Center" Foreground="#FFFAFAFA" Height="40"/>
                <control:Chooser Name="ReverseZ" ToolTip="如果Z方向位移台的示数增加方向和规定的Z轴的正向不同，则要勾选此选项" Width="40" Height="20" Margin="5"/>
                <control:DecoratedButton Margin="23,5,5,5" ToolTip="坐标系规定" Click="ShowAxisRelateInform" FontWeight="Thin" FontSize="14" Background="#FF383838" Foreground="#FFFAFAFA" MoveInColor="#FF424242" PressedColor="#FF39393A" MoveInForeground="#FFFAFAFA" PressedForeground="#FFFAFAFA" Height="40" Width="40" IconSource="/图片资源/information.png"/>
            </WrapPanel>
        </control:AutoScrollViewer.Content>
    </control:AutoScrollViewer>
    <Grid Grid.Row="1" Grid.Column="2">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <control:DecoratedButton TextAreaRatio="0" IconMargin="8" FontWeight="Thin" FontSize="14" Height="30" Width="100" Background="#FF383838" Foreground="#FFFAFAFA" MoveInColor="#FF424242" PressedColor="#FF39393A" MoveInForeground="#FFFAFAFA" PressedForeground="#FFFAFAFA" IconSource="/图片资源/play.png"/>
        <control:DecoratedButton TextAreaRatio="0" FontWeight="Thin" FontSize="14" Height="30" Width="100" Background="#FF383838" Foreground="#FFFAFAFA" MoveInColor="#FF424242" PressedColor="#FF39393A" MoveInForeground="#FFFAFAFA" PressedForeground="#FFFAFAFA" IconSource="/图片资源/stop.png" Grid.Column="2"/>
        <control:DecoratedButton TextAreaRatio="0" IconMargin="8" FontWeight="Thin" FontSize="14" Height="30" Width="100" Background="#FF383838" Foreground="#FFFAFAFA" MoveInColor="#FF424242" PressedColor="#FF39393A" MoveInForeground="#FFFAFAFA" PressedForeground="#FFFAFAFA" IconSource="/图片资源/pause.png" Grid.Column="1"/>
    </Grid>
    <control:AutoScrollViewer Margin="10" Name="scrollviewer" Grid.Column="1" Grid.Row="2" Grid.RowSpan="1">
        <control:AutoScrollViewer.Content>
            <WrapPanel Width="{Binding ElementName=scrollviewer,Path=ActualWidth}">
                <Border Name="BorderX" MouseLeftButtonUp="OpenWindow" Width="{Binding ElementName=scrollviewer,Path=ActualWidth}" Height="100" CornerRadius="10" BorderThickness="2" Background="#FF2E2E2E" BorderBrush="#FF0076FF">
                    <Grid Margin="10">
                        <Grid.RowDefinitions>
                            <RowDefinition Height=".5*"/>
                            <RowDefinition Height=".5*"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition/>
                            <ColumnDefinition Width="80"/>
                        </Grid.ColumnDefinitions>
                        <Label Name="XProgress" Content="0%" FontSize="18" Grid.RowSpan="2" Grid.Column="1" HorizontalAlignment="Center" HorizontalContentAlignment="Center" FontWeight="Bold" VerticalContentAlignment="Center" Foreground="#FFFAFAFA"/>
                        <Image Name="XOk" Height="30" Visibility="Hidden" Grid.RowSpan="2" Grid.Column="1" Width="30" Stretch="UniformToFill" Source="/图片资源/ok.png"/>
                        <Label Content="X方向扫描" FontSize="18" Grid.Column="0" HorizontalAlignment="Left" HorizontalContentAlignment="Center" FontWeight="Bold" VerticalContentAlignment="Center" Foreground="#FFFAFAFA"/>
                        <StackPanel Grid.Row="1" Grid.Column="0" Orientation="Horizontal">
                            <Label Content="状态：" VerticalAlignment="Stretch" Margin="30,0,0,0" FontSize="18" Grid.Column="0" HorizontalAlignment="Left" HorizontalContentAlignment="Center" FontWeight="Bold" VerticalContentAlignment="Center" Foreground="#FFFAFAFA"/>
                            <TextBlock Name="XState" VerticalAlignment="Stretch" Margin="30,0,0,0" FontSize="18" Grid.Column="0" HorizontalAlignment="Left"  FontWeight="Bold" Foreground="#FFFAFAFA"/>
                            <TextBlock Name="XLoc" VerticalAlignment="Stretch" Margin="30,0,0,0" FontSize="18" Grid.Column="0" HorizontalAlignment="Left"  FontWeight="Bold" Foreground="#FFFAFAFA"/>
                        </StackPanel>
                    </Grid>
                </Border>
                <Border Name="BorderY" MouseLeftButtonUp="OpenWindow" Width="{Binding ActualWidth, ElementName=scrollviewer}" Height="100" CornerRadius="10" BorderThickness="2" Background="#FF2E2E2E" BorderBrush="#FF0076FF">
                    <Grid Margin="10">
                        <Grid.RowDefinitions>
                            <RowDefinition Height=".5*"/>
                            <RowDefinition Height=".5*"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition/>
                            <ColumnDefinition Width="80"/>
                        </Grid.ColumnDefinitions>
                        <Label Name="YProgress" Content="0%" FontSize="18" Grid.RowSpan="2" Grid.Column="1" HorizontalAlignment="Center" HorizontalContentAlignment="Center" FontWeight="Bold" VerticalContentAlignment="Center" Foreground="#FFFAFAFA"/>
                        <Image Name="YOk" Height="30" Visibility="Hidden" Grid.RowSpan="2" Grid.Column="1" Width="30" Stretch="UniformToFill" Source="/图片资源/ok.png"/>
                        <Label Content="Y方向扫描" FontSize="18" Grid.Column="0" HorizontalAlignment="Left" HorizontalContentAlignment="Center" FontWeight="Bold" VerticalContentAlignment="Center" Foreground="#FFFAFAFA"/>
                        <StackPanel Grid.Row="1" Grid.Column="0" Orientation="Horizontal">
                            <Label Content="状态：" VerticalAlignment="Stretch" Margin="30,0,0,0" FontSize="18" Grid.Column="0" HorizontalAlignment="Left" HorizontalContentAlignment="Center" FontWeight="Bold" VerticalContentAlignment="Center" Foreground="#FFFAFAFA"/>
                            <TextBlock x:Name="YState" VerticalAlignment="Stretch" Margin="30,0,0,0" FontSize="18" Grid.Column="0" HorizontalAlignment="Left"  FontWeight="Bold" Foreground="#FFFAFAFA"/>
                            <TextBlock Name="YLoc" VerticalAlignment="Stretch" Margin="30,0,0,0" FontSize="18" Grid.Column="0" HorizontalAlignment="Left"  FontWeight="Bold" Foreground="#FFFAFAFA"/>
                        </StackPanel>
                    </Grid>
                </Border>
                <Border Name="BorderZ" MouseLeftButtonUp="OpenWindow" Width="{Binding ActualWidth, ElementName=scrollviewer}" Height="100" CornerRadius="10" BorderThickness="2" Background="#FF2E2E2E" BorderBrush="#FF0076FF">
                    <Grid Margin="10">
                        <Grid.RowDefinitions>
                            <RowDefinition Height=".5*"/>
                            <RowDefinition Height=".5*"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition/>
                            <ColumnDefinition Width="80"/>
                        </Grid.ColumnDefinitions>
                        <Label Name="ZProgress" Content="0%" FontSize="18" Grid.RowSpan="2" Grid.Column="1" HorizontalAlignment="Center" HorizontalContentAlignment="Center" FontWeight="Bold" VerticalContentAlignment="Center" Foreground="#FFFAFAFA"/>
                        <Image Name="ZOk" Height="30" Visibility="Hidden" Grid.RowSpan="2" Grid.Column="1" Width="30" Stretch="UniformToFill" Source="/图片资源/ok.png"/>
                        <Label Content="Z方向扫描" FontSize="18" Grid.Column="0" HorizontalAlignment="Left" HorizontalContentAlignment="Center" FontWeight="Bold" VerticalContentAlignment="Center" Foreground="#FFFAFAFA"/>
                        <StackPanel Grid.Row="1" Grid.Column="0" Orientation="Horizontal">
                            <Label Content="状态：" VerticalAlignment="Stretch" Margin="30,0,0,0" FontSize="18" Grid.Column="0" HorizontalAlignment="Left" HorizontalContentAlignment="Center" FontWeight="Bold" VerticalContentAlignment="Center" Foreground="#FFFAFAFA"/>
                            <TextBlock x:Name="ZState" VerticalAlignment="Stretch" Margin="30,0,0,0" FontSize="18" Grid.Column="0" HorizontalAlignment="Left"  FontWeight="Bold" Foreground="#FFFAFAFA"/>
                            <TextBlock Name="ZLoc" VerticalAlignment="Stretch" Margin="30,0,0,0" FontSize="18" Grid.Column="0" HorizontalAlignment="Left"  FontWeight="Bold" Foreground="#FFFAFAFA"/>
                            <TextBlock Name="ZDistance" VerticalAlignment="Stretch" Margin="30,0,0,0" FontSize="18" Grid.Column="0" HorizontalAlignment="Left"  FontWeight="Bold" Foreground="#FFFAFAFA"/>
                        </StackPanel>
                    </Grid>
                </Border>
                <Border Name="BorderAngle" MouseLeftButtonUp="OpenWindow" Width="{Binding ActualWidth, ElementName=scrollviewer}" Height="100" CornerRadius="10" BorderThickness="2" Background="#FF2E2E2E" BorderBrush="#FF0076FF">
                    <Grid Margin="10">
                        <Grid.RowDefinitions>
                            <RowDefinition Height=".5*"/>
                            <RowDefinition Height=".5*"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition/>
                            <ColumnDefinition Width="80"/>
                        </Grid.ColumnDefinitions>
                        <Label Name="AngleProgress" Content="0%" FontSize="18" Grid.RowSpan="2" Grid.Column="1" HorizontalAlignment="Center" HorizontalContentAlignment="Center" FontWeight="Bold" VerticalContentAlignment="Center" Foreground="#FFFAFAFA"/>
                        <Image Name="AngleOk" Height="30" Visibility="Hidden" Grid.RowSpan="2" Grid.Column="1" Width="30" Stretch="UniformToFill" Source="/图片资源/ok.png"/>
                        <Label Content="角度扫描" FontSize="18" Grid.Column="0" HorizontalAlignment="Left" HorizontalContentAlignment="Center" FontWeight="Bold" VerticalContentAlignment="Center" Foreground="#FFFAFAFA"/>
                        <StackPanel Grid.Row="1" Grid.Column="0" Orientation="Horizontal">
                            <Label Content="状态：" VerticalAlignment="Stretch" Margin="30,0,0,0" FontSize="18" Grid.Column="0" HorizontalAlignment="Left" HorizontalContentAlignment="Center" FontWeight="Bold" VerticalContentAlignment="Center" Foreground="#FFFAFAFA"/>
                            <TextBlock x:Name="AngleState" VerticalAlignment="Stretch" Margin="30,0,0,0" FontSize="18" Grid.Column="0" HorizontalAlignment="Left"  FontWeight="Bold" Foreground="#FFFAFAFA"/>
                            <TextBlock x:Name="AngleTheta" VerticalAlignment="Stretch" Margin="30,0,0,0" FontSize="18" Grid.Column="0" HorizontalAlignment="Left"  FontWeight="Bold" Foreground="#FFFAFAFA"/>
                            <TextBlock x:Name="AnglePhi" VerticalAlignment="Stretch" Margin="30,0,0,0" FontSize="18" Grid.Column="0" HorizontalAlignment="Left"  FontWeight="Bold" Foreground="#FFFAFAFA"/>
                        </StackPanel>
                    </Grid>
                </Border>
                <Border Name="BorderCheck" MouseLeftButtonUp="OpenWindow" Width="{Binding ActualWidth, ElementName=scrollviewer}" Height="100" CornerRadius="10" BorderThickness="2" Background="#FF2E2E2E" BorderBrush="#FF0076FF">
                    <Grid Margin="10">
                        <Grid.RowDefinitions>
                            <RowDefinition Height=".5*"/>
                            <RowDefinition Height=".5*"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition/>
                            <ColumnDefinition Width="80"/>
                        </Grid.ColumnDefinitions>
                        <Label Name="CheckProgress" Content="0%" FontSize="18" Grid.RowSpan="2" Grid.Column="1" HorizontalAlignment="Center" HorizontalContentAlignment="Center" FontWeight="Bold" VerticalContentAlignment="Center" Foreground="#FFFAFAFA"/>
                        <Image Name="CheckOk" Height="30" Visibility="Hidden" Grid.RowSpan="2" Grid.Column="1" Width="30" Stretch="UniformToFill" Source="/图片资源/ok.png"/>
                        <Label Content="角度结果检查" FontSize="18" Grid.Column="0" HorizontalAlignment="Left" HorizontalContentAlignment="Center" FontWeight="Bold" VerticalContentAlignment="Center" Foreground="#FFFAFAFA"/>
                        <StackPanel Grid.Row="1" Grid.Column="0" Orientation="Horizontal">
                            <Label Content="状态：" VerticalAlignment="Stretch" Margin="30,0,0,0" FontSize="18" Grid.Column="0" HorizontalAlignment="Left" HorizontalContentAlignment="Center" FontWeight="Bold" VerticalContentAlignment="Center" Foreground="#FFFAFAFA"/>
                            <TextBlock x:Name="CheckState" VerticalAlignment="Stretch" Margin="30,0,0,0" FontSize="18" Grid.Column="0" HorizontalAlignment="Left"  FontWeight="Bold" Foreground="#FFFAFAFA"/>
                        </StackPanel>
                    </Grid>
                </Border>
            </WrapPanel>
        </control:AutoScrollViewer.Content>
    </control:AutoScrollViewer>
</odmr_lab:PageBase>
