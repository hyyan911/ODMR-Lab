                        <controls:ViewerTemplate Descriptions="轮廓点坐标Y" DataType="Double" DataDisplayWidth="*" Editable="True"/>
                    </controls:ListViewer.DataTemplate>
                    <controls:ListViewer.ItemContextMenu>
